   "source": [
    "# lattice can be created via two methods: \n",
    "# create_lattice ('triangular, square, honeycomb') or create_custom_lattice using csv file\n",
    "latt.create_lattice('square', 1, 10)\n",
    "# latt.create_custom_lattice(\"coords.csv\") \n",
    "latt.plot_lattice()"
   ]
  },
  {
   "cell_type": "code",
