    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from HB_lattice import *\n",
    "\n",
    "# g_factor is important only if Zeeman term will be included\n",
    "latt = HB_lattice(g_factor = 2)"
   ]
  },
  {
